                                            <input class="form-control" id="value_daugia_@dg.id_daugia" style="margin-top:7%" placeholder="Giá của bạn.." />
                                            <button type="submit" onclick="Auction(@dg.id_daugia)" class="btn btn-warning" style="margin-top:4%">Đặt Giá</button>


                                            @{
