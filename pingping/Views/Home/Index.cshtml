                    })*@
                    //Auction
                    function Auction(id_daugia) {
                        var value = $('#value_daugia_' + id_daugia).val();
                        var value_max = Number($('#value_max_' + id_daugia).val());
                        //alert(value);
                        if (value != "") {
                            if (isNaN(value)) {

                                alert("Giá trị nhập phải là số.");
                            }
                            else {
                                //success
                                if (value >= (value_max + 5000)) {
                                    //alert(value_max);
                                    $.ajax({
                                        url: "/Home/Auction?dg=" + id_daugia + "&&value=" + parseFloat(value),
                                        type: "GET",
                                        // data: "Trending Item",
                                        contentType: "application/json;charset=utf-8",
                                        dataType: "json",
                                        success: function (result) {
                                            if (result == 0) {
                                                alert("Bạn Cần Đăng Nhập!");
                                            }
                                            else if (result == 1) {
                                                alert("Đấu Giá Thành Công!");
                                                location.reload();
                                            }
                                            else {
                                                alert("Đấu Giá Không Thành Công!");
                                            }
                                        },
                                        error: function (errormessage) {
                                            alert(errormessage.responseText);
                                        }
                                    });
                                }
                                else {
                                    alert("Bước giá phải từ 5,000 VNĐ và Lớn hơn giá cao nhất!");
                                }
                            }
                        }
                        else {
                            alert("Bạn cần nhập giá trị đấu giá.")
                        }

                    }
                </script>
                
            }
        }
        <hr />
        sum = 0;  name = "";
    }
}
